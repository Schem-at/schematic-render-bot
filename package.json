{
	"name": "schemat-render",
	"version": "1.0.0",
	"description": "Schematic rendering service for schemat.io",
	"main": "dist/app.js",
	"type": "module",
	"scripts": {
		"setup-libs": "cd libs && rm -rf schematic-renderer && rm -rf ../node_modules/schematic-renderer && rm -rf ../frontend/node_modules/schematic-renderer && ls *.tgz | head -1 | xargs -I {} tar -xzf {} && mv package schematic-renderer && mkdir -p ../node_modules && cp -r schematic-renderer ../node_modules/ && mkdir -p ../frontend/node_modules && cp -r schematic-renderer ../frontend/node_modules/",
		"clean-libs": "rm -rf libs/schematic-renderer node_modules/schematic-renderer frontend/node_modules/schematic-renderer",
		"build:frontend": "cd frontend && bun run build && cp -r dist ../dist-frontend",
		"build:backend": "bun run setup-libs && tsc",
		"build": "bun run build:frontend && bun run build:backend",
		"dev:backend": "bun --env-file=.env src/app.ts",
		"dev:frontend": "cd frontend && bun run dev",
		"dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
		"start": "bun --env-file=.env dist/app.js"
	},
	"dependencies": {
		"connect-timeout": "^1.9.0",
		"cors": "^2.8.5",
		"discord.js": "^14.14.1",
		"dotenv": "^16.5.0",
		"express": "^4.18.2",
		"helmet": "^7.1.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nucleation": "^0.1.48",
		"puppeteer": "^24.10.0",
		"sharp": "^0.34.5",
		"synthase": "^1.0.0",
		"three": "^0.176.0"
	},
	"devDependencies": {
		"@types/connect-timeout": "^1.9.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/morgan": "^1.9.9",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.9.0",
		"concurrently": "^8.2.2",
		"nodemon": "^3.0.1",
		"tsx": "^4.4.0",
		"typescript": "^5.2.2"
	},
	"keywords": [
		"minecraft",
		"schematic",
		"render",
		"api",
		"discord"
	],
	"author": "schemat.io",
	"license": "MIT"
}
